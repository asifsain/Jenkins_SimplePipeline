node{
    stage('Checkout'){
	git 'https://github.com/asifsain/Jenkins_SimplePipeline.git'
        sh 'echo "This is the checkout stage of Simple Pipeline"';
    }
    stage('Build'){
        sh 'echo "This is the build stage of Simple Pipeline"';
    }
    stage('Deploy'){
        sh 'echo "This is the deploy  stage of Simple Pipeline"';
    }


}
